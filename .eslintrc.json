{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "kentcdodds",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": [".env", "node_modules/", ".husky/", "*.json", "downloader/downloads/**", ".DS_Store", ".gitignore"],
    "rules": {
        "consistent-return": 0,
        "no-await-in-loop": 0
    }
}
